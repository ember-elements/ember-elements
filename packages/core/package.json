{
  "name": "@ember-elements/core",
  "version": "0.0.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/pranavliag/ember-elements.git",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:scss:fix": "stylelint app/styles/*.scss addon/components/*.scss addon/components/**/*.scss addon/components/**/**/*.scss --fix",
    "lint:scss": "stylelint app/styles/*.scss addon/components/*.scss addon/components/**/*.scss addon/components/**/**/*.scss",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:prettier:fix": "prettier --write .",
    "lint:prettier": "yarn prettier --check .",
    "lint:types": "tsc --skipLibCheck --noEmit",
    "lint:js:overide": "yarn eslint . --ext .ts --ext .js",
    "types:build": "tsc --build",
    "types:clean": "tsc --build --clean",
    "types:rebuild": "tsc --build --clean && yarn tsc --build",
    "types:watch": "tsc --build --clean && yarn tsc --build --watch",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:try-one": "ember try:one",
    "test:ember-compatibility": "ember try:each",
    "storybook": "start-storybook -p 6006 -s dist,.storybook/static",
    "build-storybook": "ember build --environment test && build-storybook -s dist,.storybook/static"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": "yarn eslint --fix --quiet --cache",
    "*.hbs": "yarn ember-template-lint --fix",
    "*.scss": "yarn stylelint --fix --cache",
    "*.js": "eslint --cache --fix",
    ".{ts,js,scss,hbs}": "yarn prettier --check "
  },
  "dependencies": {
    "ember-cli-babel": "^8.0.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-node-assets": "^0.2.2",
    "ember-cli-sass": "^10.0.1",
    "ember-in-element-polyfill": "^1.0.1",
    "popper.js": "^1.16.1",
    "sass": "^1.66.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.22.10",
    "@babel/plugin-proposal-decorators": "^7.22.10",
    "@ember-elements/icons": "0.0.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/render-modifiers": "^2.1.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.2.0",
    "@embroider/test-setup": "^3.0.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@nullvoxpopuli/eslint-configs": "^3.2.2",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/ember": "^6.3.12",
    "@storybook/ember-cli-storybook": "^0.4.0",
    "@tsconfig/ember": "^3.0.0",
    "@types/ember": "^4.0.4",
    "@types/ember-qunit": "^6.1.1",
    "@types/ember-resolver": "^9.0.0",
    "@types/ember__application": "^4.0.6",
    "@types/ember__array": "^4.0.4",
    "@types/ember__component": "^4.0.14",
    "@types/ember__controller": "^4.0.6",
    "@types/ember__debug": "^4.0.4",
    "@types/ember__destroyable": "^4.0.2",
    "@types/ember__engine": "^4.0.5",
    "@types/ember__error": "^4.0.3",
    "@types/ember__object": "^4.0.6",
    "@types/ember__polyfills": "^4.0.2",
    "@types/ember__routing": "^4.0.13",
    "@types/ember__runloop": "^4.0.4",
    "@types/ember__service": "^4.0.3",
    "@types/ember__string": "^3.0.11",
    "@types/ember__template": "^4.0.2",
    "@types/ember__test": "^4.0.2",
    "@types/ember__test-helpers": "^2.9.1",
    "@types/ember__utils": "^4.0.3",
    "@types/htmlbars-inline-precompile": "^1.0.1",
    "@types/qunit": "^2.19.6",
    "@types/rsvp": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-ember-modules-api-polyfill": "^3.5.0",
    "babel-plugin-htmlbars-inline-precompile": "^5.3.1",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.0",
    "ember-auto-import": "^2.6.3",
    "ember-cli": "~5.2.0",
    "ember-cli-clean-css": "^2.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-update": "^1.0.1",
    "ember-css-transitions": "^4.4.0",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^8.0.0",
    "ember-qunit": "^7.0.0",
    "ember-raf-scheduler": "^0.3.0",
    "ember-resolver": "^11.0.1",
    "ember-source": "~5.2.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.11.2",
    "ember-try": "^3.0.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-decorator-position": "^5.0.0",
    "eslint-plugin-ember": "^11.10.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-qunit": "^8.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "4.3.8",
    "lint-staged": "11.0.0",
    "loader.js": "^4.7.0",
    "prettier": "^3.0.2",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "sass-lint": "^1.13.1",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.0.2",
    "stylelint-scss": "^3.19.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2"
  },
  "peerDependencies": {
    "ember-source": ">= 4.0.0"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "volta": {
    "node": "18.13.0",
    "yarn": "1.22.10"
  }
}
