{
  "name": "@ember-elements/core",
  "version": "0.0.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/pranavliag/ember-elements.git",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:scss:fix": "stylelint app/styles/*.scss addon/components/*.scss addon/components/**/*.scss addon/components/**/**/*.scss --fix",
    "lint:scss": "stylelint app/styles/*.scss addon/components/*.scss addon/components/**/*.scss addon/components/**/**/*.scss",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:prettier:fix": "prettier --write .",
    "lint:prettier": "yarn prettier --check .",
    "lint:types": "tsc --skipLibCheck --noEmit",
    "lint:js:overide": "yarn eslint . --ext .ts --ext .js",
    "types:build": "tsc --build",
    "types:clean": "tsc --build --clean",
    "types:rebuild": "tsc --build --clean && yarn tsc --build",
    "types:watch": "tsc --build --clean && yarn tsc --build --watch",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:try-one": "ember try:one",
    "test:ember-compatibility": "ember try:each",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean",
    "storybook": "start-storybook -p 6006 -s dist,.storybook/static",
    "build-storybook": "ember build --environment test && build-storybook -s dist,.storybook/static"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": "yarn eslint --fix --quiet --cache",
    "*.hbs": "yarn ember-template-lint --fix",
    "*.scss": "yarn stylelint --fix --cache",
    "*.js": "eslint --cache --fix",
    ".{ts,js,scss,hbs}": "yarn prettier --check "
  },
  "dependencies": {
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-typescript": "^5.2.1",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-in-element-polyfill": "^1.0.1",
    "sass": "^1.35.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@ember-elements/icons": "0.0.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/render-modifiers": "^1.0.2",
    "@ember/test-helpers": "^2.1.4",
    "@glimmer/component": "^1.0.3",
    "@glimmer/tracking": "^1.0.3",
    "@nullvoxpopuli/eslint-configs": "^1.3.2",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/ember": "^6.3.12",
    "@storybook/ember-cli-storybook": "^0.4.0",
    "@tsconfig/ember": "^3.0.0",
    "@types/ember": "^4.0.4",
    "@types/ember-qunit": "^6.1.1",
    "@types/ember-resolver": "^9.0.0",
    "@types/ember__application": "^4.0.6",
    "@types/ember__array": "^4.0.4",
    "@types/ember__component": "^4.0.14",
    "@types/ember__controller": "^4.0.6",
    "@types/ember__debug": "^4.0.4",
    "@types/ember__destroyable": "^4.0.2",
    "@types/ember__engine": "^4.0.5",
    "@types/ember__error": "^4.0.3",
    "@types/ember__object": "^4.0.6",
    "@types/ember__polyfills": "^4.0.2",
    "@types/ember__routing": "^4.0.13",
    "@types/ember__runloop": "^4.0.4",
    "@types/ember__service": "^4.0.3",
    "@types/ember__string": "^3.0.11",
    "@types/ember__template": "^4.0.2",
    "@types/ember__test": "^4.0.2",
    "@types/ember__test-helpers": "^2.9.1",
    "@types/ember__utils": "^4.0.3",
    "@types/htmlbars-inline-precompile": "^1.0.1",
    "@types/qunit": "^2.19.6",
    "@types/rsvp": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.28.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-ember-modules-api-polyfill": "^3.5.0",
    "babel-plugin-htmlbars-inline-precompile": "^5.3.1",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.10.1",
    "ember-cli": "~3.24.0",
    "ember-cli-babel": "^8.0.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-cli-update": "^1.0.1",
    "ember-css-transitions": "^2.1.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.0.3",
    "ember-popper": "^0.11.3",
    "ember-qunit": "^5.1.1",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.24.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.15.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-decorator-position": "2.2.29",
    "eslint-plugin-ember": "^10.1.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "4.3.8",
    "lint-staged": "11.0.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "qunit": "^2.13.0",
    "qunit-dom": "^1.6.0",
    "sass-lint": "^1.13.1",
    "stylelint": "^13.13.1",
    "stylelint-scss": "^3.19.0",
    "typescript": "^5.1.6"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "volta": {
    "node": "18.13.0",
    "yarn": "1.22.10"
  }
}
