<div
  class={{rw this.getClassName this.SPINNER this.getIntent this.getNoSpin}}
  ...attributes
>
  <div class={{this.SPINNER_ANIMATION}}>
    <svg
      width={{this.getSvgSize}}
      height={{this.getSvgSize}}
      viewBox={{this.viewBox}}
      stroke-width={{this.strokeWidth}}
    >
      <path class={{this.SPINNER_TRACKClass}} d={{this.SPINNER_TRACK}}></path>
      <path
        stroke-dasharray={{this.strokeDasharray}}
        stroke-dashoffset={{this.getStrokeOffset}}
        pathLength={{this.PATH_LENGTH}}
        class={{this.SPINNER_HEAD}}
        d={{this.SPINNER_TRACK}}
      ></path>
    </svg>
  </div>
</div>